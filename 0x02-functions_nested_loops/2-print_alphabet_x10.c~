#include "holberton.h"
/**
 *print_alphabet - print alphabet
 */
void print_alphabet(void)
{
char az;
int i;
for (i = 0; i < 10; i++)
{
for (az = 'a'; az <= 'z'; az++)
{
_putchar(az);
}
_putchar('\n');
}
}
/**
 * main - Entry point
 *
 * Return: Always 0 (Success)
 */

int main(void)
{
print_alphabet();
return (0);
}
